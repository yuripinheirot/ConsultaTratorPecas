                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select                                                                                   
ivd.produto,                                                                             
ivd.descricao,                                                                           
ivd.numero,                                                                              
ivd.qtd as quantidade,                                                                   
cast(ivd.PrecoVenda as numeric(18,2)) as PrecoUnitario,                                  
